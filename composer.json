{
    "name": "learncodeweb/pdf-popup-viewer-wp",
    "description": "PDF POPUP VIEWER is a very simple plugin with and without POPUP.",
    "homepage": "https://learncodeweb.com/",
    "type": "wordpress-plugin",
    "keywords": [
        "WP Plugin",
        "BS",
        "HTML",
        "CSS",
        "Free",
        "wordpress",
        "Doc Viewer",
        "Portfolio",
        "Responsive",
        "learncodeweb.com"
    ],
    "authors": [
        {
            "name": "Khalid Zaid Bin [Mian Zaid]",
            "email": "zaid@learncodeweb.com"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/LearnCodeWeb/pdf-popup-viewer-wp/issues"
    },
    "require": {
        "php": ">=7.1.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-0": {
            "Acme": "src/"
        }
    },
    "extra": {
        "installer-paths": {
            "wp-content/mu-plugins/{$name}/": [
                "wpackagist-plugin/pdf-popup-viewer-wp"
            ],
            "wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}